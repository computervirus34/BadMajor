@model IEnumerable<BadMajor.ReportViewModel>

<table class="table table-bordered" style="font-size:12px;">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.creddesc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.state)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.instnm)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.major_grouping_desc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.major)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.cost)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.earnings)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.roi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.years_to_recoup_cost)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.grade)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.equivalent)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.creddesc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.state)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.instnm)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.major_grouping_desc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.major)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.cost)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.earnings)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.roi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.years_to_recoup_cost)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.grade)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.equivalent)
        </td>
    </tr>
}

</table>
